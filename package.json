{
  "name": "moe-picture",
  "version": "0.1.0",
  "main": ".electron.js",
  "homepage": ".",
  "private": true,
  "build": {
    "appId": "your.id",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "server/**/*",
      "src/config.js",
      ".electron.js",
      "package.json"
    ],
    "mac": {
      "category": "your.app.category.type",
      "icon": "public/img/MoePicture.png"
    },
    "win": {
      "icon": "public/img/MoePicture.png"
    },
    "linux": {
      "icon": "public/img/MoePicture.png"
    },
    "extraMetadata": {
      "main": ".electron.js"
    }
  },
  "dependencies": {
    "axios": "^0.24.0",
    "react-uwp": "^1.2.31",
    "request": "^2.88.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-builder": "npm run-script build && electron-builder ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^4.0.4",
    "electron-builder": "^20.38.5",
    "electron-packager": "^13.1.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-scripts": "^2.1.5"
  }
}
